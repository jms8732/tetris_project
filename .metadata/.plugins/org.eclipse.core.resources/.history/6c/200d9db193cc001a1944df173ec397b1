package genetic_tetris;

public class Weight {
	int number = 0;
	int line = 0;
	long score;
	double[] variation;

	public Weight(int n, int l, long s, double[] w) {
		this.number = n;
		this.line = l;
		this.score = s;
		this.variation = new double[9];

		for (int i = 0; i < 9; i++) {
			this.variation[i] = w[i];
		}

	}

}