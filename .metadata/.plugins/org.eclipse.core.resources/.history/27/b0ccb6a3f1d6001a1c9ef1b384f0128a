
public class test {
	public static void main(String[] args) {
		int [] array = {1,2,3,4,5,6,7};
		
		int [] temp = make(array);
		
		temp[0] = 7;
		temp[1] = 3;
		
		for(int i : array)
			System.out.print(i + " ");
		
		for(int i : temp)
			System.out.print(i + " ");
	}
	
	private static int [] make(int [] array) {
		return new int[] {array[0],array[2]};
	}
}
