package genetic_tetris;

import java.util.*;
import java.io.*;

/*
 * 664มู : 7.729 4.579 3.201 2.851 8.467 3.576 10.293 1.159 -0.305 
 */

public class Generic {
	private Weight[] w;
	private int size;

	public Generic(int count) {
		this.size = count;
		w = new Weight[this.size];
	
		for(int i =0 ; i <w.length ; i++) {
			double hw = Math.random() * (2*10) - 10;
			double bw = Math.random() * (2*10) -10;
			double aw = Math.random() * (2*10) - 10;
			double cw = Math.random() * (2*10) - 10;
			
			w[i] = new Weight(i+1,0,0,hw,bw,cw,aw);
		}
	}

	public Weight[] get_weight() {
		return w;

	}
	
	public void cross_over() {
		
	}

}
